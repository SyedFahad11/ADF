{
	"name": "Deduplication",
	"properties": {
		"type": "Flowlet",
		"typeProperties": {
			"sources": [],
			"sinks": [],
			"transformations": [
				{
					"name": "RemoveDuplicates"
				},
				{
					"name": "RankAs1"
				},
				{
					"name": "select"
				},
				{
					"name": "Input"
				},
				{
					"name": "Output"
				}
			],
			"scriptLines": [
				"input(output(",
				"          customer_id as integer,",
				"          last_updated_ts as timestamp",
				"     ),",
				"     order: 0,",
				"     allowSchemaDrift: true) ~> Input",
				"Input window(over(customer_id),",
				"     desc(last_updated_ts, true),",
				"     rank = rowNumber()) ~> RemoveDuplicates",
				"RemoveDuplicates filter(rank==1) ~> RankAs1",
				"RankAs1 select(mapColumn(",
				"          customer_id,",
				"          last_updated_ts",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select",
				"select output(mapColumn(",
				"          each(match(/* All input columns */ true()),",
				"               /* Input name */ $$ = $$)",
				"     )) ~> Output"
			]
		}
	}
}